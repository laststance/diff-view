{
  "name": "diff-view",
  "productName": "Diff View",
  "version": "1.0.0",
  "description": "An offline desktop application for GitHub-style text comparison and diff visualization",
  "homepage": "https://github.com/laststance/diff-view",
  "repository": {
    "type": "git",
    "url": "https://github.com/laststance/diff-view.git"
  },
  "bugs": {
    "url": "https://github.com/laststance/diff-view/issues"
  },
  "main": ".vite/build/main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "prettier": "prettier --write \"src/**/*.{ts,tsx,js,jsx,css,json}\"",
    "typecheck": "tsc --noEmit",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage --run",
    "test:unit": "vitest --run tests/unit",
    "test:integration": "vitest --run tests/integration",
    "test:all": "pnpm test:unit && pnpm test:integration && pnpm test:e2e",
    "test:e2e": "playwright test --headed",
    "test:e2e:headless": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "build:all": "pnpm build:windows && pnpm build:macos && pnpm build:linux",
    "build:windows": "electron-forge make --platform=win32",
    "build:macos": "electron-forge make --platform=darwin",
    "build:linux": "electron-forge make --platform=linux",
    "build:current": "electron-forge make",
    "package:all": "pnpm package:windows && pnpm package:macos && pnpm package:linux",
    "package:windows": "electron-forge package --platform=win32",
    "package:macos": "electron-forge package --platform=darwin",
    "package:linux": "electron-forge package --platform=linux",
    "package:current": "electron-forge package",
    "prebuild": "pnpm lint && pnpm typecheck && pnpm test",
    "postinstall": "electron-rebuild"
  },
  "keywords": [],
  "author": {
    "name": "Ryota Murakami",
    "email": "dojce1048@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^7.9.0",
    "@electron-forge/maker-deb": "^7.9.0",
    "@electron-forge/maker-dmg": "^7.9.0",
    "@electron-forge/maker-rpm": "^7.9.0",
    "@electron-forge/maker-squirrel": "^7.9.0",
    "@electron-forge/maker-zip": "^7.9.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.9.0",
    "@electron-forge/plugin-fuses": "^7.9.0",
    "@electron-forge/plugin-vite": "^7.9.0",
    "@electron-forge/shared-types": "^7.9.0",
    "@electron/fuses": "^1.8.0",
    "@electron/rebuild": "^4.0.1",
    "@playwright/test": "^1.55.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/electron-squirrel-startup": "^1.0.2",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "electron": "38.2.0",
    "eslint": "^8.57.1",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-vitest": "^0.5.4",
    "jsdom": "^27.0.0",
    "playwright": "^1.55.1",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.9.2",
    "vite": "^5.4.20",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@git-diff-view/file": "^0.0.32",
    "@git-diff-view/react": "^0.0.32",
    "@types/react-window": "^1.8.8",
    "diff": "^8.0.2",
    "electron-squirrel-startup": "^1.0.1",
    "lucide-react": "^0.544.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.9.3",
    "react-window": "^2.2.0",
    "zustand": "^5.0.8"
  },
  "volta": {
    "node": "22.21.0"
  },
  "packageManager": "pnpm@10.20.0+sha512.cf9998222162dd85864d0a8102e7892e7ba4ceadebbf5a31f9c2fce48dfce317a9c53b9f6464d1ef9042cba2e02ae02a9f7c143a2b438cd93c91840f0192b9dd"
}
